	        keep="true"
	        />


	<!-- startup action - load the first scene -->
	<action name="startup" autorun="onstart">
		if(startscene === null OR !scene[get(startscene)], copy(startscene,scene[0].name); );
		loadscene(get(startscene), null, MERGE);
		if(startactions !== null, startactions() );
	</action>


	<action name="bgsnd_action" autorun="onstart">
		playsound(bgsnd, 'sound/bg.mp3', 0);
		stopsound(bgsnd);
	</action>

	
	<!-- start playing the sound -->
	<events name="currentpano" onnewpano="playsound(bgsnd, '%SWFPATH%/sound/bg.mp3', 0);" onremovepano="stopsound(bgsnd);" />

	<!-- a button to pause/play the sound -->
